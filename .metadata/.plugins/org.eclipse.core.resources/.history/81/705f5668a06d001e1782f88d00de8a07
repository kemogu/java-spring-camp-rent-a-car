package kodlama.io.rentACar.entities.concretes;

import jakarta.persistence.Table;
import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Table(name = "brads")
@Data
@AllArgsConstructor
@NoArgsConstructor
public class Brand {
	private int id;
	private String name;
	
	
}
